{
  "name": "mongoose-partial-dumper",
  "version": "0.4.4",
  "description": "A package to help mongoose users to partially dump models of your db",
  "main": "build/index.cjs.js",
  "types": "build/index.d.ts",
  "module": "src/index.ts",
  "unpkg": "build/index.umd.js",
  "author": "Afonso AraÃºjo Neto",
  "license": "ISC",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "delete:node_modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "jest": "jest --no-cache --detectOpenHandles",
    "prepare": "husky install",
    "babel": "babel-node --extensions \".es6,.js,.es,.mjs,.ts\"",
    "build": "npm run clear && yarn rollup --config",
    "build:linux:admin": "yarn build && chmod +x build/cli.cjs.js && chmod +x build/index.cjs.js",
    "clear": "rimraf ./build",
    "release:patch": "yarn babel ./scripts/release/release-patch"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@endemolshinegroup/cosmiconfig-typescript-loader": "^3.0.2",
    "cosmiconfig": "^8.1.3",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "faker": "^5.5.3",
    "generate-changelog": "^1.8.0",
    "lodash": "4.17.21",
    "mongoose": "^5.12.0",
    "semver": "^7.3.5",
    "simple-git": "^2.48.0",
    "yargs": "^17.0.0-candidate.13",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/faker": "5.5.8",
    "@types/jest": "26.0.20",
    "@types/mongoose": "5.10.3",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-jest": "29.5.0",
    "eslint": "^7.22.0",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "5.1.3",
    "jest": "^26.6.3",
    "jest-junit": "16.0.0",
    "lint-staged": ">=10",
    "mongodb-memory-server": "8.12.2",
    "prettier": "~2.8.8",
    "rollup": "2.78.0",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "5.0.4"
  },
  "bin": {
    "partial": "./build/cli.cjs.js"
  },
  "repository": "https://github.com/Streeterxs/mongoose-partial-dump",
  "lint-staged": {
    "*.{ts}": "eslint --fix",
    "*.spec.{js,ts}": "eslint --fix",
    "*.{js,ts,css,md}": "prettier --write"
  }
}
