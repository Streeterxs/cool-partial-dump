{
  "name": "mongoose-partial-dumper",
  "version": "0.4.1",
  "description": "A package to help mongoose users to partially dump models of your db",
  "main": "build/index.cjs.js",
  "types": "build/index.d.ts",
  "module": "src/index.ts",
  "unpkg": "build/index.umd.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "delete:node_modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "jest": "jest --no-cache --detectOpenHandles",
    "prepare": "husky install",
    "prebuild": "eslint --fix tsconfig.json --fix",
    "babel": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\"",
    "build": "npm run clear && yarn rollup --config && tsc --emitDeclarationOnly",
    "build:linux:admin": "yarn build && chmod +x build/cli.cjs.js && chmod +x build/index.cjs.js",
    "clear": "rimraf ./build"
  },
  "author": "Afonso AraÃºjo Neto",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@endemolshinegroup/cosmiconfig-typescript-loader": "^3.0.2",
    "cosmiconfig": "^7.0.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "faker": "^5.5.3",
    "mongoose": "^5.12.0",
    "yargs": "^17.0.0-candidate.13",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/faker": "5.5.8",
    "@types/jest": "26.0.20",
    "@types/mongoose": "5.10.3",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "babel-jest": "26.6.3",
    "eslint": "^7.22.0",
    "eslint-plugin-jest": "^24.3.2",
    "husky": "5.1.3",
    "jest": "^26.6.3",
    "jest-junit": "12.0.0",
    "lint-staged": ">=10",
    "mongodb-memory-server": "^6.9.6",
    "prettier": "~2.2.1",
    "rollup": "^2.56.3",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "4.2.3"
  },
  "bin": {
    "partial": "./build/cli.cjs.js"
  },
  "repository": "https://github.com/Streeterxs/mongoose-partial-dump",
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.spec.{js,ts}": "eslint --fix",
    "*.{js,ts,css,md}": "prettier --write"
  }
}
